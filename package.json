  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.1",
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.3",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2",
    "tailwindcss-rtl": "^0.7.3"
  }
